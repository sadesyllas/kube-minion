{
  "$schema": "https://raw.githubusercontent.com/sadesyllas/kube-minion/main/kube-minion.schema.json",
  "loadBalancers": [
    {
      "namespace": "default",
      "resourceType": "deployments",
      "name": "nginx",
      "port": 8080,
      "targetPort": 80
    }
  ],
  "socatTunnels": [
    {
      "protocol": "tcp",
      "listeningPort": 8888,
      "connectHost": "localhost",
      "connectPort": 8080
    }
  ],
  "minikubeMounts": [
    {
      "hostPath": "/tmp/foo",
      "minikubePath": "/data/foo"
    }
  ],
  "defaultSocatConnectHost": "192.168.160.1"
}
